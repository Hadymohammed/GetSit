<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - GetSit</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/GetSit.styles.css" asp-append-version="true" />
    <!-- Google Font -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
          rel="stylesheet">

    <!-- Bootstrap  -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <style>
        .body {
            font-family: 'Poppins', sans-serif;
            height: 200rem;
        }

        .navbar {
            box-sizing: border-box;
            background: #FFFFFF !important;
            border-bottom: 1px solid rgba(102, 102, 102, 0.25);
        }

        .wrapper {
            display: grid;
            grid-template-columns: 1fr 1fr 1fr 1fr;
        }
        .flex {
            display: flex;
        }

        .avatar {
            border-radius: 50%;
            width: 40px;
            margin-left: 40px;
        }

        .links-container-edit {
            display: flex;
            flex-direction: row;
            align-items: flex-end;
            padding: 0px;
            gap: 40px;
            /* width: 58px; */
            /* height: 24px; */
            /* font-family: 'Poppins'; */
            font-style: normal;
            font-weight: 500;
            font-size: 16px;
            /* line-height: 24px; */
            color: #111111;
        }

        .main {
            /* position: absolute; */
            height: 100vh;
            background-color: #ffffff;
            margin-left: 5%;
        }

        .Filters-label {
            /* position: absolute; */
            /* width: 102px; */
            /* height: 24px; */
            left: 50px;
            top: 123px;
            /* font-style: normal; */
            font-weight: 700;
            font-size: 16px;
            line-height: 24px;
            /* display: flex; */
            align-items: center;
            text-align: center;
            color: #212121;
        }


        .btn-edit {
            /* position: absolute; */
            width: 150px;
            height: 34px;
            margin-left: 23px;
            /* margin-top: 118px; */
            font-style: normal;
            font-weight: 600;
            /* font-size: 14px; */
            /* line-height: 14px; */
            /* display: flex; */
            align-items: center;
            text-align: center;
            letter-spacing: -0.1px;
            /* color: #212121; */
        }

        .Clear-Filters {
            /* position: absolute; */
            /* width: 81px; */
            height: 14px;
            left: 1260px;
            top: 130px;
            /* font-family: 'Inter'; */
            font-style: normal;
            font-weight: 600;
            font-size: 14px;
            /* line-height: 14px; */
            margin-left: 10%;
        }

        .edit-container {
            display: flex;
            justify-content: space-between;
        }

        .search-input-edit {
            /* position: absolute; */
            /* height: 40px; */
            /* left: 50px; */
            /* right: 99px; */
            /* margin-top: 28px; */
            /* position: absolute; */
            /* left: 38px; */
            /* top: 13px; */
            /* font-family: 'Inter'; */
            background: #FFFFFF;
            border-radius: 8px;
            width: 95%;
            font-style: normal;
            font-weight: 400;
            font-size: 16px;
            line-height: 20px;
            display: flex;
            align-items: center;
            color: #424242;
            border-color: #00000052;
            border: solid 1px #0000004a;
            /* margin-left: 2%; */
            padding: 10px;
        }

        .Filters-container {
            /* position: absolute; */
            /* height: 80%; */
            background-color: #ffffff;
            padding-top: 118px;
            /* padding-left: 50px; */
            /* padding-top: 3rem; */
            display: flex;
            gap: 3rem;
            margin-bottom: 2rem;
        }

        .Spaces-near {
            font-size: 19px;
            font-weight: 700;
            line-height: 28px;
            text-align: left;
            margin-top: 50px;
        }

        .card-edit {
            width: 18rem;
            border: white;
            margin-left: 4rem;
        }

            .card-edit img {
                border-radius: 0%;
            }

        .space-b {
            justify-content: space-between;
        }
    </style>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">GetSit</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - GetSit - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <!-- Bootstrap js -->
    @*<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.7/dist/umd/popper.min.js"
            integrity="sha384-zYPOMqeu1DAVkHiLqWBUTcbYfZ8osu1Nd6Z89ify25QV9guujx43ITvfi12/QExE"
            crossorigin="anonymous"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.min.js"
            integrity="sha384-Y4oOpwW3duJdCWv5ly8SCFYWqFDsfob/3GkgExXKV4idmbt98QcxXYs9UoXAB7BZ"
            crossorigin="anonymous"></script>
</body>
</html>
